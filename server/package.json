{
  "name": "realms",
  "description": "An updated webxr port of realmsvr",
  "author": "Daniel Esteban Nombela",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "cert": "openssl req -x509 -newkey rsa:2048 -nodes -sha256 -subj /CN=localhost -keyout localhost-key.pem -out localhost-cert.pem",
    "protocol": "pbjs -t static-module -w es6 -o ../client/core/protocol.js --no-comments messages.proto",
    "postinstall": "npm run cert",
    "start": "cross-env TLS_CERT=localhost-cert.pem TLS_KEY=localhost-key.pem node main.js"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "callsite-record": "^4.1.3",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "express-ws": "^4.0.0",
    "gl-matrix": "^3.3.0",
    "helmet": "^4.3.1",
    "hsv-rgb": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.11",
    "mongoose-slug-updater": "^3.2.3",
    "namor": "^2.0.2",
    "nocache": "^2.1.0",
    "noisejs": "^2.1.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "protobufjs": "^6.10.2",
    "puppeteer": "^5.5.0",
    "uuid": "^8.3.2"
  }
}

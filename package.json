{
  "name": "realms",
  "description": "A webxr port of realmsvr",
  "author": "Daniel Esteban Nombela",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "cert": "openssl req -x509 -newkey rsa:2048 -nodes -sha256 -subj /CN=localhost -keyout localhost-key.pem -out localhost-cert.pem",
    "postinstall": "npm run cert",
    "start": "sirv --dev --single --host 0.0.0.0 --cert localhost-cert.pem --key localhost-key.pem --http2 ."
  },
  "devDependencies": {
    "sirv-cli": "^1.0.10"
  }
}
